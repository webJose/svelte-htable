{
	"name": "svelte-htable",
	"version": "0.0.8",
	"license": "MIT",
	"description": "Svelte component that creates a hierarchical (recursive) table based on hierarchical data.",
	"keywords": [
		"svelte",
		"table",
		"hierarchical table",
		"recursive"
	],
	"author": "José Pablo Ramírez Vargas <webJose@gmail.com>",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/webJose/svelte-htable.git"
	},
	"bugs": {
		"url": "https://github.com/webJose/svelte-htable/issues"
	},
	"homepage": "https://github.com/webJose/svelte-htable#readme",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"module": "./dist/index.js",
	"files": [
		"dist"
	],
	"peerDependencies": {
		"svelte": "^3.54.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "1.6.0",
		"@sveltejs/package": "^2.0.0",
		"publint": "^0.1.9",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.2.0",
		"vitest": "^0.25.3"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module"
}
